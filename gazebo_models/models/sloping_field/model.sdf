        </surface>
        <laser_retro>-1.0</laser_retro>
      </collision>
    </link>

    <link name="polygonum">
      <visual name="polygonum">
        <geometry>
          <mesh>
            <uri>model://sloping_field_assets/meshes/polygonum.obj</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>model://sloping_field_assets/materials/polygonum.material</uri>
            <name>polygonum</name>
          </script>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://sloping_field_assets/meshes/polygonum.obj</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <collide_without_contact>true</collide_without_contact>
            <collide_bitmask>0x00</collide_bitmask>
          </contact>
        </surface>
        <laser_retro>-2.0</laser_retro>
      </collision>
    </link>

    <link name="taraxacum">
      <visual name="taraxacum">
        <geometry>
          <mesh>
            <uri>model://sloping_field_assets/meshes/taraxacum.obj</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>model://sloping_field_assets/materials/taraxacum.material</uri>
            <name>taraxacum</name>
          </script>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://sloping_field_assets/meshes/taraxacum.obj</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <collide_without_contact>true</collide_without_contact>
            <collide_bitmask>0x00</collide_bitmask>
          </contact>
        </surface>
        <laser_retro>-3.0</laser_retro>
      </collision>
    </link>

    <link name="stones">
      <visual name="stones">
        <geometry>
          <mesh>
            <uri>model://sloping_field_assets/meshes/stones.obj</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>model://sloping_field_assets/materials/stones.material</uri>
            <name>stones</name>
          </script>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://sloping_field_assets/meshes/stones.obj</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <collide_without_contact>true</collide_without_contact>
            <collide_bitmask>0x00</collide_bitmask>
          </contact>
        </surface>
        <laser_retro>-0.5</laser_retro>
      </collision>
    </link>

    <link name="stem_collisions">
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://sloping_field_assets/meshes/stem_collisions.obj</uri>
          </mesh>
        </geometry>
        <!-- <surface>
          <contact>
            <collide_without_contact>true</collide_without_contact>
            <collide_bitmask>0x00</collide_bitmask>
          </contact>
        </surface> -->
        <surface>
          <contact>
            <!-- bitmask: stems(4), weeds(3), crops(2), elements(1), ground(0) -->
            <collide_bitmask>0x10</collide_bitmask>
            <collide_without_contact>false</collide_without_contact>
            <collide_without_contact_bitmask>0x10</collide_without_contact_bitmask>
            <!-- Fake collide_without_contact by changing ode config. -->
            <!-- This is required to detect collision with 'contacts' topic. -->
            <ode>
              <soft_cfm>0</soft_cfm>
              <soft_erp>0</soft_erp>
              <kp>0</kp>
              <kd>0</kd>
              <max_vel>0.01</max_vel>
              <min_depth>0.0</min_depth>
            </ode>
          </contact>
        </surface>
        <laser_retro>-5.0</laser_retro>
      </collision>
    </link>
  </model>
</sdf>
